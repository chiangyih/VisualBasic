Dim PtrAddress As Long          '印表機埠變數
Dim DataOutValue As Integer  '資料埠變數
Dim CtrlOutValue As Integer    '控制埠變數

Private Sub Command1_Click()
  
End Sub

Private Sub Command2_Click()
  DIO_OutputByte PtrAddress, 0    '印表機埠資料線清空
  End
End Sub

Private Sub CtrlOut_Click(Index As Integer)
    If CtrlOut(Index).Value = 1 Then
    CtrlOutValue = CtrlOutValue Or 2 ^ Index
  Else
    CtrlOutValue = CtrlOutValue And (255 - 2 ^ Index)
  End If
  DIO_OutputByte PtrAddress + 2, CtrlOutValue

End Sub

Private Sub DataOut_Click(Index As Integer)
  If DataOut(Index).Value = 1 Then
    DataOutValue = DataOutValue Or 2 ^ Index
  Else
    DataOutValue = DataOutValue And (255 - 2 ^ Index)
  End If
  DIO_OutputByte PtrAddress, DataOutValue
End Sub

Private Sub Form_Load()
 PtrAddress = &H378
 OutValue = 0
 DIO_OutputByte PtrAddress, OutValue
End Sub
